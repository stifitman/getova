<%- model_class = Company -%>
<div class="page-header">
<h1><%=t '.title', :default => [:'helpers.titles.new', 'New %{model}'], :model => model_class.model_name.human.titleize %></h1>
</div>

  <%= form_tag '/companies', :multipart => true do %>
    <%= label_tag(:name, "Name:") %>
    <%= text_field_tag(:name) %>
    <label for="file">Upload Linked HTML File</label> <%= file_field_tag "file" %>
    <%= submit_tag %>
  <% end %>
