<h1>Tanet Use-Case</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tanets.each do |tanet| %>
      <tr>
        <td><%= tanet.data %></td>
        <td><%= link_to 'Show', tanet %></td>
        <td><%= link_to 'Edit', edit_tanet_path(tanet) %></td>
        <td><%= link_to 'Destroy', tanet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tanet', new_tanet_path %>
